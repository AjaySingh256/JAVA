// Variable
public class variable {
    public static void main(String[] args) {
         int a= 10;
         int b = 25;
         int sum = a+b;
       System.out.println(sum);

         int diff = b-a;
         System.out.println(diff);

         int mul = a*b;
         System.out.println(mul);

         int div = b/a;
         System.out.println(div);

         int mod = b%a;
         System.out.println(mod);

    }
}
//***OUTPUT***
// 35
// 15 
// 250
// 2  
// 5  
